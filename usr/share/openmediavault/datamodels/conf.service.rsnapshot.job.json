{
	"type": "config",
	"id": "conf.service.rsnapshot.job",
	"title": "rsnapshot job",
	"queryinfo": {
		"xpath": "//services/rsnapshot/jobs/job",
		"iterable": true,
		"idproperty": "uuid"
	},
	"properties": {
		"uuid": {
			"type": "string",
			"format": "uuidv4"
		},
		"enable": {
			"type": "boolean",
			"default": true
		},
		"comment": {
			"type": "string",
			"default": ""
		},
		"targetfolderref": {
			"type": "string",
			"format": "uuidv4"
		},
		"targetfoldername": {
			"type": "string",
			"default": ""
		},
		"onefs": {
			"type": "boolean",
			"default": false
		},
		"sourcefolderref": {
			"type": "string",
			"format": "uuidv4"
		},
		"sourcefoldername": {
			"type": "string",
			"default": ""
		},
		"hourly": {
			"type": "integer",
			"minimum": 0,
			"maximum": 50,
			"default": 0
		},
		"daily": {
			"type": "integer",
			"minimum": 0,
			"maximum": 50,
			"default": 7
		},
		"weekly": {
			"type": "integer",
			"minimum": 0,
			"maximum": 50,
			"default": 4
		},
		"monthly": {
			"type": "integer",
			"minimum": 0,
			"maximum": 50,
			"default": 3
		},
		"yearly": {
			"type": "integer",
			"minimum": 0,
			"maximum": 50,
			"default": 0
		},
		"numtries": {
			"type": "integer",
			"minimum": 1,
			"maximum": 65535,
			"default": 1
		},
		"gid": {
			"type": "string",
			"default": ""
		},
		"erroroutputonly": {
			"type": "boolean",
			"default": true
		},
		"rsyncargs": {
			"type": "string",
			"default": ""
		}
	}
}
