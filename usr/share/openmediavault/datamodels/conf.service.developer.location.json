{
	"type": "config",
	"id": "conf.service.developer.location",
	"title": "Developer Location",
	"queryinfo": {
		"xpath": "//services/developer/locations/location",
		"iterable": true,
		"idproperty": "uuid"
	},
	"properties": {
		"uuid": {
			"type": "string",
			"format": "uuidv4"
		},
		"name": {
			"type": "string",
			"default": ""
		},
		"remotehost": {
			"type": "string",
			"default": ""
		},
		"remotepath": {
			"type": "string",
			"default": ""
		},
		"port": {
			"type": "integer",
			"minimum": 1,
			"maximum": 65535,
			"default": 22
		},
		"username": {
			"type": "string",
			"default": ""
		},
		"password": {
			"type": "string",
			"default": ""
		}
	}
}
